  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start:admin": "yarn install && FRONTEND_PACKAGE=admin node scripts/start.js",
    "build:admin": "yarn install && FRONTEND_PACKAGE=admin node scripts/build.js",
    "lint:admin": "yarn install && eslint 'packages/admin/src/{components,constants,containers,context,helpers,reducers,services,store,utils}/**/*.{tsx,ts}' --fix",
    "start:frontend": "yarn install && FRONTEND_PACKAGE=frontend node scripts/start.js",
    "build:frontend": "yarn install && FRONTEND_PACKAGE=frontend node scripts/build.js",
    "lint:frontend": "yarn install && eslint 'packages/frontend/src/{components,constants,containers,context,helpers,reducers,services,store,utils}/**/*.{tsx,ts}' --fix",
    "test": "yarn install && node scripts/test.js --env=jsdom",
    "test:ci": "yarn install && CI=true node scripts/test.js --env=jsdom",
    "test:console": "yarn install && node scripts/test.js --testPathPattern=\"./packages/podder-management-console/src/\" --env=jsdom",
    "precommit": "lint-staged",
    "generate": "plop --plopfile generators/index.js",
    "storybook": "yarn install && start-storybook -p 6006"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": "eslint"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  }
